{
  "name": "FilmTicket",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "build:e2e:ios": "detox build --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "build:e2e:android": "detox build --configuration android.emu.debug"
  },
  "dependencies": {
    "@faker-js/faker": "^9.0.2",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@types/lodash": "^4.17.9",
    "@types/react-native": "^0.72.8",
    "@types/react-redux": "^7.1.34",
    "lodash": "^4.17.21",
    "react": "18.3.1",
    "react-native": "0.75.3",
    "react-native-pager-view": "^6.4.1",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "^3.34.0",
    "react-native-vector-icons": "^10.2.0",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.3",
    "@react-native/eslint-config": "0.75.3",
    "@react-native/metro-config": "0.75.3",
    "@react-native/typescript-config": "0.75.3",
    "@testing-library/react-native": "^12.7.2",
    "@types/react": "^18.3.9",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "detox": "^20.27.2",
    "eslint": "^8.19.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "redux-mock-store": "^1.5.4",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=18"
  },
  "detox": {
    "test-runner": "jest",
    "configurations": {
      "ios": {
        "type": "ios.simulator",
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/FilmTicket.app",
        "build": "xcodebuild -workspace ios/FilmTicket.xcworkspace -scheme FilmTicket -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "device": {
          "type": "iPhone 16"
        }
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "name": "Pixel_3_XL_API_33"
      }
    }
  }
}
